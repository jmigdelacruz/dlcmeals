    "@vue/test-utils": "^2.4.1",
    "esbuild": "^0.25.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "firebase-tools": "^12.4.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "sortablejs": "^1.15.6",
